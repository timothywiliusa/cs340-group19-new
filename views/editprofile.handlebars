<h1>Edit Profile</h1>
{{#each search}}
<form action='/profile/edit'>
	<h1 id="username">{{username}}</h1>
	<h2>Address:</h2>

	<label for="zip">ZIP Code:</label>
	<input type="text" id="zip" name="zip" size="28" value={{zip}}></input><br>

	<label for="planet">Planet # from star:</label>
	<input type="text" id="planet" name="planet" size="28" value={{planetNum}}></input><br>

	<label for="xcoord">X Coordinate:</label>
	<input type="text" id="xcoord" name="xcoord" size="28" value={{Xcoord}}></input><br>

	<label for="ycoord">Y Coordinate:</label>
	<input type="text" id="ycoord" name="ycoord" size="28" value={{Ycoord}}></input><br>

	<label for="po">PO Box #:</label>
	<input type="text" id="po" name="po" size="28" value={{POBox}}></input><br>

    <input type='submit' value='submit'>
</form>
{{/each}}